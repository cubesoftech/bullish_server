generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model agents {
  id             String       @id @unique @default(cuid())
  masteragentsId String
  memberId       String
  referralCode   String
  royalty        Int
  masteragents   masteragents @relation(fields: [masteragentsId], references: [id], onDelete: Cascade)
  members        members[]

  @@index([masteragentsId], map: "agents_masteragentsId_fkey")
}

model users {
  id             String     @id @default(cuid())
  password       String?
  name           String?
  email          String?    @unique
  email_verified DateTime?
  image          String?
  accounts       accounts[]
  sessions       sessions[]
}

model accounts {
  id                  String  @id
  user_id             String
  type                String
  provider            String  @unique
  provider_account_id String  @unique
  refresh_token       String? @db.Text
  access_token        String? @db.Text
  expires_at          Int?
  token_type          String?
  scope               String?
  id_token            String? @db.Text
  session_state       String?
  users               users   @relation(fields: [user_id], references: [id], onDelete: Cascade, map: "Accounts_user_id_fkey")

  @@index([user_id], map: "Accounts_user_id_fkey")
}

model agents_withdrawals {
  id        String                    @id @unique
  amount    Int
  status    agents_withdrawals_status
  role      agents_withdrawals_role
  createdAt DateTime                  @default(now())
  updatedAt DateTime
  membersId String?
  members   members?                  @relation(fields: [membersId], references: [id], onDelete: Cascade)

  @@index([membersId], map: "agents_withdrawals_membersId_fkey")
}

model announcement {
  id        String   @id
  title     String
  content   String   @db.VarChar(5000)
  createdAt DateTime @default(now())
  updatedAt DateTime
}

model bank {
  id   String @id @unique
  name String @unique
}

model inject_setting {
  id         String   @id
  status     Boolean
  createdAt  DateTime @default(now())
  userId     String
  multiplier Int
}

model inquiries {
  id              String   @id
  title           String
  content         String   @db.VarChar(5000)
  answer          String   @db.VarChar(5000)
  memberId        String
  createdAt       DateTime @default(now())
  updatedAt       DateTime
  alreadyAnswered Boolean  @default(false)
  membersId       String?
  members         members? @relation(fields: [membersId], references: [id], onDelete: Cascade, map: "Inquiries_membersId_fkey")

  @@index([membersId], map: "Inquiries_membersId_fkey")
}

model masteragents {
  id           String   @id @unique
  memberId     String
  referralCode String?
  royalty      Int
  agents       agents[]
}

model member_login_logs {
  id          String    @id
  membersID   String    @unique
  lastSignIn  DateTime?
  lastSignOut DateTime?
  members     members   @relation(fields: [membersID], references: [id], onDelete: Cascade, map: "Member_Login_Logs_fkey")
}

model members {
  id                    String                  @id @unique
  email                 String
  role                  members_role            @default(USER)
  name                  String?
  password              String?
  confirmpassword       String?
  nickname              String?
  birthdate             String?
  phonenumber           String?
  accountholder         String?
  accountnumber         String?
  bank                  String?
  referrer              String?
  balance               Int?                    @default(0)
  agentsId              String?
  createdAt             DateTime?               @default(now())
  status                Boolean                 @default(false)
  multiplier            Int?                    @default(0)
  lastOnline            DateTime?               @default(now())
  force                 Boolean                 @default(false)
  switchBet             Boolean                 @default(false)
  maxBet                Boolean                 @default(false)
  clearToDeposit        Boolean?                @default(false)
  clearToWithdraw       Boolean?                @default(false)
  eurusd_trade          members_eurusd_trade    @default(eurusd)
  gold_trade            members_gold_trade      @default(gold)
  nasdaq_trade          members_nasdaq_trade    @default(nasdaq)
  nvda_trade            members_nvda_trade      @default(nvda)
  pltr_trade            members_pltr_trade      @default(pltr)
  tsla_trade            members_tsla_trade      @default(tsla)
  agents_withdrawals    agents_withdrawals[]
  inquiries             inquiries[]
  member_login_logs     member_login_logs?
  agents                agents?                 @relation(fields: [agentsId], references: [id])
  membertrades          membertrades[]
  transaction           transaction[]
  transaction_approvals transaction_approvals[]
  refresh_tokens        refresh_tokens?

  @@index([agentsId], map: "members_agentsId_fkey")
}

model membertrades {
  id               String            @id @unique
  timeExecuted     DateTime          @default(now())
  trade            Boolean
  tradeAmount      Int
  tradePNL         Int
  membersId        String
  type             membertrades_type
  remainingBalance Int?
  origTradeAmount  Int?
  members          members           @relation(fields: [membersId], references: [id], onDelete: Cascade, map: "MemberTrades_membersId_fkey")

  @@index([membersId], map: "MemberTrades_membersId_fkey")
}

model recenttrades {
  id           String            @id @unique
  type         recenttrades_type
  tradinghours DateTime
  result       Boolean
}

model sessions {
  id            String   @id
  session_token String   @unique
  user_id       String
  expires       DateTime
  users         users    @relation(fields: [user_id], references: [id], onDelete: Cascade, map: "Sessions_user_id_fkey")

  @@index([user_id], map: "sessions_user_id_fkey")
}

model sitesettings {
  id            String  @id
  returnOnWin   Float
  oneMinLock    Int
  threeMinLock  Int
  fiveMinLock   Int
  eurusd        Boolean
  gold          Boolean
  nasdaq        Boolean
  nvda          Boolean
  pltr          Boolean
  tsla          Boolean
  minimumAmount Int
  maintenance   Boolean @default(false)
}

model transaction {
  id          String             @id @unique
  type        transaction_type
  amount      Int
  status      transaction_status
  description String
  membersId   String
  createdAt   DateTime           @default(now())
  updatedAt   DateTime
  hasReplied  Boolean?           @default(false)
  reply       String?
  members     members            @relation(fields: [membersId], references: [id], onDelete: Cascade, map: "Transaction_membersId_fkey")

  @@index([membersId], map: "Transaction_membersId_fkey")
}

model transaction_approvals {
  id          String                       @id
  membersID   String
  reply       String?
  status      transaction_approvals_status
  dateCreated DateTime
  dateUpdated DateTime
  type        transaction_approvals_type
  members     members                      @relation(fields: [membersID], references: [id], onDelete: Cascade, map: "Transaction_Approvals_fkey")

  @@index([membersID], map: "Transaction_Approvals_fkey")
}

model verificationtokens {
  identifier String   @unique
  token      String   @unique
  expires    DateTime
}

model refresh_tokens {
  id        String   @id @default(cuid())
  memberId  String   @unique
  token     String   @unique
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now()) @updatedAt
  members   members  @relation(fields: [memberId], references: [id], onDelete: Cascade, map: "Refresh_Tokens_MemberId_fkey")
}

enum recenttrades_type {
  nasdaq_1_min
  nasdaq_3_mins
  nasdaq_5_mins
  gold_1_min
  gold_3_mins
  gold_5_mins
  eurusd_1_min
  eurusd_3_mins
  eurusd_5_mins
  pltr_1_min
  pltr_3_mins
  pltr_5_mins
  tsla_1_min
  tsla_3_mins
  tsla_5_mins
  nvda_1_min
  nvda_3_mins
  nvda_5_mins
}

enum transaction_type {
  deposit
  withdrawal
}

enum agents_withdrawals_status {
  pending
  completed
  failed
  TBA
}

enum members_role {
  ADMIN
  MASTER_AGENT
  AGENT
  USER
}

enum agents_withdrawals_role {
  ADMIN
  MASTER_AGENT
  AGENT
  USER
}

enum transaction_status {
  pending
  completed
  failed
  TBA
}

enum transaction_approvals_status {
  pending
  accepted
  rejected
}

enum membertrades_type {
  nasdaq_1_min
  nasdaq_3_mins
  nasdaq_5_mins
  gold_1_min
  gold_3_mins
  gold_5_mins
  eurusd_1_min
  eurusd_3_mins
  eurusd_5_mins
  pltr_1_min
  pltr_3_mins
  pltr_5_mins
  tsla_1_min
  tsla_3_mins
  tsla_5_mins
  nvda_1_min
  nvda_3_mins
  nvda_5_mins
}

enum transaction_approvals_type {
  deposit
  withdrawal
}

enum members_eurusd_trade {
  nasdaq
  gold
  eurusd
  pltr
  tsla
  nvda
}

enum members_gold_trade {
  nasdaq
  gold
  eurusd
  pltr
  tsla
  nvda
}

enum members_nasdaq_trade {
  nasdaq
  gold
  eurusd
  pltr
  tsla
  nvda
}

enum members_nvda_trade {
  nasdaq
  gold
  eurusd
  pltr
  tsla
  nvda
}

enum members_pltr_trade {
  nasdaq
  gold
  eurusd
  pltr
  tsla
  nvda
}

enum members_tsla_trade {
  nasdaq
  gold
  eurusd
  pltr
  tsla
